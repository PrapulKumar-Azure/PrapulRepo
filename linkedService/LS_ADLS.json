{
	"name": "LS_ADLS",
	"properties": {
		"parameters": {
			"datalakeUrl": {
				"type": "string"
			},
			"tenantId": {
				"type": "string"
			},
			"servicePrincipleId": {
				"type": "string"
			},
			"secretName": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "AzureBlobFS",
		"typeProperties": {
			"url": "@{linkedService().datalakeUrl}",
			"tenant": "@{linkedService().tenantId}",
			"servicePrincipalId": "@{linkedService().servicePrincipleId}",
			"servicePrincipalCredentialType": "ServicePrincipalKey",
			"servicePrincipalCredential": {
				"type": "AzureKeyVaultSecret",
				"store": {
					"referenceName": "LS_AKV",
					"type": "LinkedServiceReference"
				},
				"secretName": {
					"value": "@linkedService().secretName",
					"type": "Expression"
				}
			}
		}
	},
	"type": "Microsoft.DataFactory/factories/linkedservices"
}